/* Menu */
.well-super {
 background-color: #000000;
 border: 1px solid rgba(0, 0, 0, 0.05);
 border-radius: 4px 4px 4px 4px;
 box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05) inset;
 margin-bottom: 20px;
 min-height: 20px;
 padding: 19px;
}

.well-super .arrowlistmenu .menuheader{color:#FFFFFF;}
.well-super .arrowlistmenu .openheader{
background-image: -khtml-gradient(linear, left bottom, left center, from(#FF0000), to(#000000));
background-image: -moz-linear-gradient(bottom center, #FF0000, #000000);
background-image: -ms-linear-gradient(bottom center, #FF0000, #000000);
background-image: -webkit-gradient(linear, left bottom, left center, color-stop(0%, #FF0000), color-stop(100%, #000000));
background-image: -webkit-linear-gradient(bottom center, #FF0000, #000000);
background-image: -o-linear-gradient(bottom center, #FF0000, #000000);
background-image: linear-gradient(bottom center, #FF0000, #000000);
}

.arrowlistmenu{
width: 180px; /*width of accordion menu*/
}

.arrowlistmenu .menuheader{ /*CSS class for menu headers in general (expanding or not!)*/
font: bold 14px Arial;
color: #555555;
background-image: -khtml-gradient(linear, left bottom, left center, from(#EEDC94), to(#FFFFFF));
background-image: -moz-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -ms-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -webkit-gradient(linear, left bottom, left center, color-stop(0%, #EEDC94), color-stop(100%, #FFFFFF));
background-image: -webkit-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -o-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: linear-gradient(bottom center, #EEDC94, #FFFFFF);
margin-bottom: 5px; /*bottom spacing between header and rest of content*/
text-transform: uppercase;
padding: 4px 0 4px 10px; /*header text is indented 10px*/
cursor: hand;
display:block;
cursor: pointer;
}

.arrowlistmenu .openheader{
background-image: -khtml-gradient(linear, left bottom, left center, from(#EEDC94), to(#FFFFFF));
background-image: -moz-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -ms-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -webkit-gradient(linear, left bottom, left center, color-stop(0%, #EEDC94), color-stop(100%, #FFFFFF));
background-image: -webkit-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: -o-linear-gradient(bottom center, #EEDC94, #FFFFFF);
background-image: linear-gradient(bottom center, #EEDC94, #FFFFFF);
}

.arrowlistmenu ul{ /*CSS for UL of each sub menu*/
list-style-type: none;
margin: 0;
padding: 0;
margin-bottom: 8px; /*bottom spacing between each UL and rest of content*/
}

.arrowlistmenu ul li{
padding-bottom: 2px; /*bottom spacing between menu items*/
line-height:normal;
}

.arrowlistmenu ul li .opensubheader{ /*Open state CSS for sub menu header*/
background: lightblue !important;
}

.arrowlistmenu ul li .closedsubheader{ /*Closed state CSS for sub menu header*/
background: lightgreen !important;
}

.arrowlistmenu ul.sub li a{
color: #FFFFFF;
background: url(../../assets/mz_images/admin/panah_kanan.gif) no-repeat 10px center; /*custom bullet list image*/
display: block;
padding: 2px 0;
padding-left: 19px; /*link text is indented 19px*/
text-decoration: none;
font-weight: bold;
border-bottom: 1px solid #FFFFFF;
font-size: 90%;
}

.arrowlistmenu ul li a:visited{
color: #FFFFFF;
}

.arrowlistmenu ul li a:hover{ /*hover state CSS*/
background-color: #EEDC94;
color:#000;
}

.well-super .arrowlistmenu ul li a:hover{ /*hover state CSS*/
background-color: #FFF;
color:#000;
}

.arrowlistmenu ul li a.subexpandable:hover{ /*hover state CSS for sub menu header*/
background: lightblue;
}

.expandable a{color:#555555;}

